{
  "name": "express-mongo-boilerplate",
  "version": "0.0.0",
  "description": "This repository is powered by jungdj",
  "main": "index.js",
  "private": true,
  "scripts": {
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "start": "node index",
    "dev": "yarn env:dev nodemon index --watch src",
    "prod": "yarn env:prod nodemon index",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/register": "^7.13.0",
    "aws-sdk": "^2.852.0",
    "axios": ">=0.21.1",
    "chalk": "^4.1.0",
    "cookie-parser": "~1.4.5",
    "cross-env": "^7.0.3",
    "debug": "~4.3.1",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "dotenv-safe": "^8.2.0",
    "express": "~4.17.3",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.1",
    "graphql": "^15.5.0",
    "helmet": "^4.4.1",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.11.18",
    "morgan": "~1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "s3-image-size": "^0.1.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  }
}
